000025 940c 000c                 	jmp RUN
                                 
                                 ; ---------------------------------------
                                 ; --- Multiplex display
                                 ; --- Uses : r16
                                 MUX:
                                 	;*** skriv rutin som handhar multiplexningen och ***
                                 	;*** utskriften till diodmatrisen . Öka SEED . ***
                                 	
                                 	; LINE (current line/row)
                                 	; PORTB (display output values)
                                 	; bit num to display num 1-7
                                 	; PORTA (display output control)
                                 	; 0-A 1-B 2-C
                                 
                                 	; shut off while changing
000027 e000                      	ldi r16, 0
000028 bb08                      	out PORTB, r16
                                 
                                 	; increase display line (control)
000029 9110 0064                 	lds r17, LINE
00002b 930f
00002c 9100 0064
00002e 9503
00002f 9300 0064
000031 910f                      	INCSRAM LINE
000032 3014                      	cpi r17, 4
000033 f408                      	brsh RESET_LINE
000034 c003                      	rjmp KEEP_LINE
                                 RESET_LINE:
000035 e000                      	ldi r16, 0
000036 9300 0064                 	sts LINE, r16
                                 KEEP_LINE:
                                 	;copy control bits to PORTA

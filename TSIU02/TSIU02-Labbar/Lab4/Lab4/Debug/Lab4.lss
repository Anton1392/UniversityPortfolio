000008 940e 00dd                 	call HW_INIT
00000a 940e 00e7                 	call WARM
                                 RUN:
00000c 940e 0076                 	call JOYSTICK
00000e 940e 010f                 	call ERASE
000010 940e 00c3                 	call UPDATE
                                 
                                 	;*** Vanta en stund sa inte spelet gar for fort ***
000012 940e 011c                 	call DELAY
                                 
                                 	;DONE: Avgor om traff
000014 9100 0060                 	lds r16, POSX
000016 9110 0062                 	lds r17, TPOSX
000018 1701                      	cp r16, r17 ; Not same X -> no hit
000019 f459                      	brne NO_HIT
00001a 9100 0061                 	lds r16, POSY
00001c 9110 0063                 	lds r17, TPOSY
00001e 1701                      	cp r16, r17 ; Not same Y -> no hit
00001f f429                      	brne NO_HIT
                                 
